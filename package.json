{
  "name": "bloutand",
  "version": "1.0.0",
  "description": "An electron app written in Typescript and React that can scan for nearby BLE devices, connect/disconnect to them and read and display values of a selected service characteristic (e.g. battery levels) from the connected device.",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "keywords": [],
  "author": {
    "name": "Shaun Saker",
    "email": "sakershaun@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@hot-loader/react-dom": "^16.8.6",
    "@jest-runner/electron": "^2.0.3",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.2",
    "@types/web-bluetooth": "^0.0.5",
    "@types/webpack-env": "^1.13.3",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "cross-env": "^5.1.3",
    "css-loader": "^3.0.0",
    "electron": "9.4.0",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.7.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^25.1.0",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.8.8",
    "source-map-loader": "^0.2.4",
    "storybook-addon-material-ui": "^0.9.0-alpha.21",
    "style-loader": "^0.23.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@types/electron-packager": "^14.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-vis": "^1.11.7",
    "styled-components": "^5.0.1"
  },
  "build": {
    "productName": "bloutand",
    "appId": "co.za.shaunsaker.bloutand",
    "mac": {
      "category": "utilities"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  }
}

{
  "name": "bloutand",
  "productName": "bloutand",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": {
    "name": "Shaun Saker",
    "email": "sakershaun@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "bloutand"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@electron-forge/cli": "6.0.0-beta.48",
    "@electron-forge/maker-deb": "6.0.0-beta.48",
    "@electron-forge/maker-rpm": "6.0.0-beta.48",
    "@electron-forge/maker-squirrel": "6.0.0-beta.48",
    "@electron-forge/maker-zip": "6.0.0-beta.48",
    "@electron-forge/plugin-webpack": "6.0.0-beta.48",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@types/styled-components": "^4.4.2",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "css-loader": "^3.0.0",
    "electron": "8.0.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-react": "^7.16.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1"
  }
}
